{
	"name": "silassare/gobl",
	"version": "2.0.0",
	"description": "Gobl allows you to use a Database Abstraction Layer (DBAL) and Object-Relational Mapping (ORM) to query your database with PHP.",
	"license": "MIT",
	"keywords": [
		"gobl",
		"ORM",
		"DBAL",
		"CRUD",
		"MySQL"
	],
	"require": {
		"php": ">=8.1",
		"ext-pdo": "*",
		"ext-json": "*",
		"ext-bcmath": "*",
		"silassare/otpl": "^2.0.0",
		"silassare/php-utils": "^1.0.0",
		"oliup/code-generator": "^1.0.0"
	},
	"require-dev": {
		"oliup/oliup-cs-php": "dev-main",
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5.9",
		"vimeo/psalm": "^5.26.1"
	},
	"homepage": "https://github.com/silassare/gobl#readme",
	"autoload": {
		"psr-4": {
			"Gobl\\": "src",
			"Gobl\\Tests\\": "tests"
		},
		"files": [
			"src/bootstrap.php"
		]
	},
	"authors": [
		{
			"name": "Emile Silas Sare",
			"email": "emile.silas@gmail.com",
			"homepage": "https://github.com/silassare"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-dev": "2.0.x-dev"
		}
	}
}
