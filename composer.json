{
	"name": "silassare/gobl",
	"version": "1.5.0",
	"description": "Gobl allows you to use a Database Abstraction Layer (DBAL) and Object-Relational Mapping (ORM) to query your database with PHP.",
	"license": "MIT",
	"keywords": [
		"gobl",
		"ORM",
		"DBAL",
		"CRUD",
		"MySql"
	],
	"require": {
		"php": ">=8.1",
		"silassare/otpl": "=1.1.9",
		"ext-pdo": "*",
		"ext-json": "*",
		"ext-bcmath": "*",
		"silassare/php-utils": "@dev"
	},
	"require-dev": {
		"oliup/oliup-cs-php": "dev-main"
	},
	"repositories": {
		"oliup/oliup-cs-php": {
			"type": "vcs",
			"url": "https://github.com/oliup-io/oliup-cs-php"
		},
		"silassare/php-utils": {
			"type": "vcs",
			"url": "https://github.com/silassare/php-utils"
		}
	},
	"homepage": "https://github.com/silassare/gobl#readme",
	"archive": {
		"exclude": [
			"/test"
		]
	},
	"autoload": {
		"psr-4": {
			"Gobl\\": "src"
		}
	},
	"authors": [
		{
			"name": "Emile Silas Sare",
			"email": "emile.silas@gmail.com",
			"homepage": "https://github.com/silassare"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
